{
  "name": "thor",
  "version": "1.0.0",
  "main": "dist/server.js",
  "repository": "https://github.com/ericdsw/hackaton-copa",
  "author": "@chris-fran<christopher.f.almanza@gmail.com>, @ericdsw<eric.desedas.w@gmail.com>",
  "license": "PROPIETARY",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "build": "yarn run compile && yarn run tslint",
    "start": "concurrently -k -p \"[{name}]\" -n \"Typescript,Node\" -c \"cyan.bold,green.bold\" \"yarn compile -w\" \"yarn nodemon dist/server.js\"",
    "serve": "node dist/server.js",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "concurrently": "^4.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  }
}
